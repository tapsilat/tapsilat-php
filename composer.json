{
    "name": "tapsilat/tapsilat-php",
    "description": "A simple PHP package that returns tapsilat",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tapsilat",
            "email": "info@tapsilat.com"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "scripts": {
        "test": "phpunit"
    },
    "autoload": {
        "psr-4": {
            "Tapsilat\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tapsilat\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
} 